{
  "printWidth": 80,
  "tabWidth": 2,
  "endOfLine": "lf",
  "semi": true,
  "trailingComma": "es5",
  "arrowParens": "avoid",
  "singleQuote": true,
  "jsxSingleQuote": true,
  "quoteProps": "as-needed",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "singleAttributePerLine": true,
  "proseWrap": "always",
  "htmlWhitespaceSensitivity": "strict",
  "plugins": ["prettier-plugin-tailwindcss"]
}
